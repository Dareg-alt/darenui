{
  "name": "@daren/ui-components",
  "version": "1.20.3",
  "repository": {
    "type": "git",
    "url": "git@github.com:darenmalfait/darenui.git",
    "directory": "packages/ui-components"
  },
  "license": "ISC",
  "sideEffects": false,
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "typings": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "run-s 'build:*'",
    "build:types": "tsc --emitDeclarationOnly --declaration --declarationDir dist/types --declarationMap --rootDir lib --outDir dist/types --sourceMap",
    "build:esm": "cross-env BABEL_ENV=esm babel lib --root-mode upward --extensions .ts,.tsx -d dist/esm --source-maps",
    "build:cjs": "cross-env BABEL_ENV=cjs babel lib --root-mode upward --extensions .ts,.tsx -d dist/cjs --source-maps",
    "clean": "rm -rf node_modules && rm -rf dist",
    "lint": "run-p 'lint:*'",
    "lint:lib": "eslint lib --ext .ts,.tsx --config ../../.eslintrc.js",
    "lint:types": "tsc --noEmit",
    "dev": "nodemon --watch lib --exec yarn build -e ts,tsx",
    "prebuild": "rimraf dist",
    "test": "jest --env=jsdom --passWithNoTests",
    "test:cov": "yarn test --coverage",
    "version": "yarn build"
  },
  "dependencies": {
    "@daren/alert": "^1.4.19",
    "@daren/button": "^2.0.8",
    "@daren/code-block": "^3.0.11",
    "@daren/form-elements": "^1.14.3",
    "@daren/grid": "^1.4.15",
    "@daren/hover-card": "^1.0.0",
    "@daren/layout-components": "^1.3.15",
    "@daren/modal": "^1.4.22",
    "@daren/navigation-list": "^1.4.3",
    "@daren/progressive-image": "^1.4.13",
    "@daren/review": "^1.3.23",
    "@daren/scroll-area": "^1.0.1",
    "@daren/spinner": "^1.1.15",
    "@daren/tabs": "^2.0.3",
    "@daren/theme": "^2.4.7",
    "@daren/toast": "^1.1.2",
    "@daren/typography": "^1.9.12",
    "@daren/utils": "^1.9.6"
  },
  "devDependencies": {
    "react": "^18.2.0"
  },
  "peerDependencies": {
    "react": ">=18.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
