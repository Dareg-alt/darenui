{
  "name": "@daren/ui-components",
  "version": "1.13.1",
  "repository": {
    "type": "git",
    "url": "git@github.com:darenmalfait/darenui.git",
    "directory": "packages/ui-components"
  },
  "license": "ISC",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "unpkg": "dist/index.global.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "lib"
  ],
  "tsup": {
    "entry": [
      "lib/index.ts"
    ],
    "format": [
      "esm",
      "cjs",
      "iife"
    ]
  },
  "scripts": {
    "build": "run-s build:*",
    "build:source": "tsup --env.NODE_ENV production --minify --sourcemap --clean --dts",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "lint": "run-p lint:*",
    "lint:lib": "eslint lib --ext .ts,.tsx --config ../../.eslintrc.js",
    "lint:types": "tsc --noEmit",
    "start": "nodemon --watch lib --exec yarn build -e ts,tsx",
    "test": "jest --env=jsdom --passWithNoTests",
    "test:cov": "yarn test --coverage",
    "version": "yarn build"
  },
  "dependencies": {
    "react": "^17.0.2"
  },
  "devDependencies": {
    "@daren/alert": "^1.1.7",
    "@daren/button": "^1.1.0",
    "@daren/code-block": "^2.2.10",
    "@daren/form-elements": "^1.8.4",
    "@daren/grid": "^1.1.14",
    "@daren/layout-components": "^1.0.11",
    "@daren/modal": "^1.2.3",
    "@daren/navigation-list": "^1.0.11",
    "@daren/progressive-image": "^1.0.15",
    "@daren/review": "^1.0.11",
    "@daren/theme": "^2.1.8",
    "@daren/typography": "^1.4.4",
    "@daren/utils": "^1.5.3"
  },
  "peerDependencies": {
    "react": ">=17.0.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
