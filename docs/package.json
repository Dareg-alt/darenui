{
  "name": "@daren/docs",
  "version": "0.0.0",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build:css": "yarn generate:css -- --minify",
    "build:remix": "remix build",
    "build": "cd .. && yarn build && cd docs && run-s build:*",
    "dev:css": "yarn generate:css -- --watch",
    "dev:remix": "remix dev",
    "dev": "run-p dev:*",
    "generate:css": "postcss styles/**/*.css --base styles --dir app/styles",
    "lint:lib": "eslint lib --ext .ts,.tsx --config ../.eslintrc.js",
    "lint:types": "tsc --noEmit",
    "lint": "run-p 'lint:*'",
    "ntl:ci": "netlify dev",
    "start": "cross-env NODE_ENV=production netlify dev"
  },
  "eslintIgnore": [
    "/netlify/functions/server",
    "/node_modules",
    "/public/build",
    "/server/build"
  ],
  "dependencies": {
    "@daren/button": "^1.3.5",
    "@daren/theme": "^2.2.0",
    "@daren/ui-components": "^1.14.5",
    "@daren/ui-core": "^2.6.2",
    "@daren/utils": "^1.6.0",
    "@heroicons/react": "^2.0.12",
    "@netlify/functions": "^1.3.0",
    "@radix-ui/react-tabs": "^1.0.0",
    "@remix-run/netlify": "^1.7.2",
    "@remix-run/node": "^1.7.2",
    "@remix-run/react": "^1.7.2",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/typography": "^0.5.7",
    "autoprefixer": "^10.4.12",
    "bcryptjs": "^2.4.3",
    "clsx": "^1.2.1",
    "postcss-import": "^15.0.0",
    "prism-react-renderer": "^1.3.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-live": "^2.4.1",
    "shiki": "^0.9.7",
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.7.2",
    "@remix-run/eslint-config": "^1.7.2",
    "@remix-run/serve": "^1.7.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/eslint": "^8.4.6",
    "@types/react-dom": "^18.0.6",
    "@types/react": "^18.0.21",
    "cross-env": "^7.0.3",
    "eslint": "^8.24.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^10.0.0",
    "postcss-loader": "^7.0.1",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=14"
  }
